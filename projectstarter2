Echo 'Creating a new github';

cd;
cd "$1/$2"
url -X POST -H "Accept: application/vnd.github+json" -H "Authorization: token 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" -H "X-GitHub-Api-Version: 2022-11-28" https://api.github.com/user/repos -d "{\"name\": \"$2\" }";

git init;
git add -A;
git commit -m 'This is an automated commit';
git branch -M main;
git remote add origin "https://github.com/NuckenMcFuggets/$2.git";
git push -u origin main;